#
# Bloomberg configuration
#
<% if !@env['env_general']['bestx']['bestxengine']['bloombergtsoxtradercode'].nil? -%>
Bloomberg.Tsox.tradercode = <%=  @env['env_general']['bestx']['bestxengine']['bloombergtsoxtradercode'] %>
<%- end -%>


#
# Tradeweb configuration
#
<% if !@env['env_general']['bestx']['bestxengine']['tradexpresstradercode'].nil? -%>
tradexpress.traderCode=<%=  @env['env_general']['bestx']['bestxengine']['tradexpresstradercode'] %>
<%- end -%>
<% if !@env['env_general']['bestx']['bestxengine']['twinvestmentdecisorcode'].nil? -%>
Tradeweb.InvestmentDecisionMakerID=<%=  @env['env_general']['bestx']['bestxengine']['twinvestmentdecisorcode'] %>
<%- end -%>
Tradeweb.InvestmentDecisionRoleQualifier=24
<% if !@env['env_general']['bestx']['bestxengine']['twexecutiondecisionmakerid'].nil? -%>
Tradeweb.ExecutionDecisionMakerID=<%=  @env['env_general']['bestx']['bestxengine']['twexecutiondecisionmakerid'] %>
<%- end -%>

#
# marketAxess configuration
#
<% if !@env['env_general']['bestx']['bestxengine']['traderpartyid'].nil? -%>
Marketaxess.Order.TraderPartyID=<%=  @env['env_general']['bestx']['bestxengine']['traderpartyid'] %>
<%- end -%>
#MiFID info for MarketAxess
<% if !@env['env_general']['bestx']['bestxengine']['mainvestmentdecisorcode'].nil? -%>
MarketAxess.InvestmentDecisionMakerID=<%=  @env['env_general']['bestx']['bestxengine']['mainvestmentdecisorcode'] %>
<%- end -%>
MarketAxess.InvestmentDecisionMakerIDSource=P
MarketAxess.InvestmentDecisionQualifier=24

<% if !@env['env_general']['bestx']['bestxengine']['maexecutiondecisionmakerid'].nil? -%>
MarketAxess.ExecutionDecisionMakerID=<%=  @env['env_general']['bestx']['bestxengine']['maexecutiondecisionmakerid'] %>
<%- end -%>


