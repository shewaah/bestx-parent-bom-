	<!-- OPERATION -->
	<class name="it.softsolutions.bestx.Operation" table="Operation">
		<id name="id" column="OperationId" access="field">
			<generator class="native" />
		</id>
		<!-- BESTX-286 -->
		<many-to-one name="owner" column="Owner" class="it.softsolutions.bestx.model.UserModel" lazy="false" cascade="none" not-null="false" />
		<many-to-one name="rfq" column="RfqId"
			class="it.softsolutions.bestx.model.Rfq" lazy="false" cascade="all" />
		<many-to-one name="order" column="OrderId"
			class="it.softsolutions.bestx.model.Order" lazy="false" cascade="all" />
		<list name="executionReports" lazy="false" cascade="all-delete-orphan">
			<key column="OperationId" />
			<index column="OperationIndex" type="int" />
			<one-to-many class="it.softsolutions.bestx.model.ExecutionReport" />
		</list>
		<list name="attempts" lazy="false" cascade="all-delete-orphan">
			<key column="OperationId" />
			<index column="OperationIndex" type="int" />
			<one-to-many class="it.softsolutions.bestx.model.Attempt" />
		</list>
		<!-- BESTX-321 -->
		<property name="lastSavedAttempt" column="LastSavedAttempt" access="field"/>
		<map name="identifiers" lazy="false" table="OperationBinding"
			access="field" cascade="all-delete-orphan">
			<key column="OperationId" not-null="true" />
			<map-key column="ExternalIdType" type="operationIdType" />
			<element type="string" column="ExternalId" not-null="true" />
		</map>
		<many-to-one name="currentState" column="OperationStateId"
			class="it.softsolutions.bestx.BaseState" lazy="false" cascade="all"
			access="field" unique="true" />
		<property name="attemptNo" column="AttemptNo" type="integer"
			access="field" />
		<property name="firstAttemptInCurrentCycle" column="FirstAttemptInCurrentCycle"
			type="integer" access="field" />
		<property name="firstValidAttemptNo" column="FirstValidAttemptNo"
			type="integer" access="field" />
		<property name="noProposalsOrderOnBook" column="NoProposalsOrderOnBook"
			access="field" />
		<property name="magnetCustomerRevoke" column="MagnetCustomerRevoke"
			access="field" />
		<property name="customerRevokeReceived" column="CustomerRevokeReceived"
			access="field" />
