[default]
DefaultApplVerID=FIX.5.0
ConnectionType=initiator
TimeZone=Europe/London

## BLOOMBERG ########################
<% if !@env['env_general']['tradestac']['ssox_fix_gateway']['sendercompid'].nil? -%>
SenderCompID=<%=  @env['env_general']['tradestac']['ssox_fix_gateway']['sendercompid'] %>
<%- end -%>
<% if !@env['env_general']['tradestac']['ssox_fix_gateway']['targetcompid'].nil? -%>
TargetCompID=<%=  @env['env_general']['tradestac']['ssox_fix_gateway']['targetcompid'] %>
<%- end -%>
<% if !@env['env_general']['tradestac']['ssox_fix_gateway']['socketconnecthost'].nil? -%>
SocketConnectHost=<%=  @env['env_general']['tradestac']['ssox_fix_gateway']['socketconnecthost'] %>
<%- end -%>
<% if !@env['env_general']['tradestac']['ssox_fix_gateway']['socketconnectport'].nil? -%>
SocketConnectPort=<%=  @env['env_general']['tradestac']['ssox_fix_gateway']['socketconnectport'] %>
<%- end -%>
#####################################

StartTime=00:00:00
EndTime=00:00:00
HeartBtInt=30
ReconnectInterval=5
RefreshOnLogon=Y
ResetOnLogon=Y
ResetOnLogout=Y

################################################################################
# Storage
################################################################################

# Directory to store sequence number and message files.
# Only used with FileStoreFactory.
# Values {valid directory for storing files, must have write access}
FileStorePath=filestore-transaction-client

# Boolean option for controlling whether the FileStore syncs to the hard drive on every write.
# Warning setting FileStoreSync=Y affects performances.
# Default {N}
# Values {N, Y}
FileStoreSync=Y

[session]
BeginString=FIXT.1.1
AppDataDictionary=FIX50-tradestac.xml
TransportDataDictionary=FIXT11-tradestac.xml
LogonTimeout=10
